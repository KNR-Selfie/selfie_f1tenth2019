<launch>

  <node pkg="selfie_stm32_bridge" type="selfie_stm32_bridge" name="stm32"/>
  <node name="lidar" pkg="urg_node" type="urg_node" output="screen"/>

  <node pkg="tf" type="static_transform_publisher" name="tf_base2imu" args="0 0 0 0 0 0 1 base_link imu 10"/>
  <node pkg="tf" type="static_transform_publisher" name="tf_base2laser" args=".06 0.08 0 1 0 0 0 base_link laser 10"/>
  <node pkg="selfie_odometry" type="selfie_odometry" name="odometry" output="screen"/>
  

</launch>
